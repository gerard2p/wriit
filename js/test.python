def numberOfPaths(a):
	if a[0][0]==0:
		return 0
	if len(a)==1 and len(a[0])==1:
		return 1
	suba = []
	subb = []
	try:
		for rows in a[1:len(a)]:
			suba.append(rows)
	except IndexError:
		suba=[]
	try:
		for rows in a:
			subb.append(rows[1:len(rows)])
	except IndexError:
		subb=[]
			
	if len(suba) > 0:
		suba = numberOfPaths( suba )
	else:
		suba=0
	if len(subb)>0 and len(subb[0]) > 0:
		subb = numberOfPaths( subb )
	else:
		subb=0
	return suba + subb
a = [ [1,1],[0,1]]
print numberOfPaths(a)